mixin breadcrumbs()
  +b.breadcrumb
    +b.container
      +b.row
        +b.col
          +b('nav')(aria-label="breadcrumb").breadcrumb
            +b('ul').breadcrumb
              +b('li').breadcrumb-item
                +b(href="/").text-white Home
              +b('li').text-white.breadcrumb-item.active Services
